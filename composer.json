{
    "name": "nacosvel/locator",
    "description": "Manages multiple instances, handling default selection, creation, caching, and extension.",
    "keywords": [
        "nacosvel",
        "locator"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/nacosvel/locator",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nacosvel/macroable": "^1.0"
    },
    "require-dev": {
        "code-lts/doctum": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\Locator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "docs": "@php vendor/bin/doctum.php update doctum.php"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
